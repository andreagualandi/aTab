{
    "name": "aTab",
    "version": "1.0",
    "manifest_version": 2,
    "description": "A new tab for chrome",
    "permissions": [
        "activeTab",
        "<all_urls>",
        "storage",
        "topSites",
        "history",
        "chrome://favicon/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}